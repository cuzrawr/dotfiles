URxvt.tabbed.autohide: true
URxvt*perl-ext: tabbed
! URxvt.font: xft:Terminus:pixelsize=12
URxvt*saveLines: 10536
URxvt*scrollBar_right:false

! #icon
URxvt*iconFile: /usr/share/icons/Faenza/apps/64/lxterminal.png

! !###
URxvt.font: xft:DejaVu Sans Mono:Regular:pixelsize=14:antialias=true
URxvt.boldFont: xft:DejaVu Sans Mono:bold:pixelsize=14:antialias=true

! #URxvt*background: white
URxvt*transparent:true
URxvt*scrollBar:false
URxvt*borderLess:false
URxvt*foreground:2  ! #Font color.

! #ursl
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:    /usr/bin/firefox
URxvt.matcher.button:   1

urxvt*shading: 72
urxvt*cursorColor: 4
urxvt*tintColor: black
urxvt*blurRadius: 4
! #urxvt*font:-*-terminus-medium-*-*-*-14-*-*-*-*-*-*-r
urxvt*geometry: 100x24
urxvt*cursorBlink: true
URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    0
URxvt.tabbed.tab-bg:    12

